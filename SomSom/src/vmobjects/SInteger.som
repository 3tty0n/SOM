SInteger = SAbstractObject (
  | value |

  initialize: anInteger = (
    value := anInteger
  )

  integer = ( ^ value )

  somClassIn: universe = (
    ^ universe integerClass
  )

  ----

  "TODO: see whether it makes sense to have a cache"
  for: anInteger = (
    ^ self new initialize: anInteger
  )
)
